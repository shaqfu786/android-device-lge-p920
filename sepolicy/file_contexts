# Sensors
/data/misc/sens/fifo_dat                                        u:object_r:sensors_device:s0
/data/misc/sens/fifo_cmd                                        u:object_r:sensors_device:s0
/data/misc/sens/profile_calib_a                                 u:object_r:sensors_device:s0
/data/misc/sens/profile_calib_m                                 u:object_r:sensors_device:s0

# radio
/data/misc/wifi/macAddress                                      u:object_r:radio_data_file:s0
/dev/xmd-.*                                                     u:object_r:radio_device:s0
/dev/xmd-tty6                                                   u:object_r:gps_device:s0

# Vibrator
/dev/tspdrv                                                     u:object_r:input_device:s0

# Hardware tunables
/sys/devices/omapdss/display0/gamma_tuning                      u:object_r:sysfs_writable:s0
/sys/devices/platform/tspdrv/nforce_timed                       u:object_r:sysfs_writable:s0
/sys/devices/omapdss/display0/nature         	                u:object_r:sysfs_writable:s0

# Firmwares
/system/etc/firmware/ducati-m3\.bin                             u:object_r:firmware_ducati:s0

# Bluetooth
